@model IEnumerable<BicycleShop.Models.User>
@{
}


<div class="col-lg-5 mx-auto">
	<table class="table table-dark table-striped">
		<thead>
			<tr>
				<th scope="col">Email</th>
				<th scope="col" style="width: 50">+</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var user in Model)
			{
				<tr>
					<td>@user.Email</td>
					<td>
						<a class="btn btn-warning" asp-action="Edit" asp-controller="Account" asp-route-userId="@user.Id">Edit</a>
					</td>
				</tr>
			}
		</tbody>
	</table>
</div>